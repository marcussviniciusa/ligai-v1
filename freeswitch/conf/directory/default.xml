<?xml version="1.0"?>
<!--
    LigAI - Directory (sem usuários internos por enquanto)
-->
<include>
  <domain name="$${domain}">
    <params>
      <param name="dial-string" value="{presence_id=${dialed_user}@${dialed_domain}}${sofia_contact(${dialed_user}@${dialed_domain})}"/>
    </params>
    <groups>
      <group name="default">
        <users>
          <!-- Adicionar usuários aqui se necessário -->
        </users>
      </group>
    </groups>
  </domain>
</include>
