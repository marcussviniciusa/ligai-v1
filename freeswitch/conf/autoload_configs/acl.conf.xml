<?xml version="1.0"?>
<configuration name="acl.conf" description="Network Lists">
  <network-lists>
    <list name="lan" default="allow">
      <node type="allow" cidr="192.168.0.0/16"/>
      <node type="allow" cidr="10.0.0.0/8"/>
      <node type="allow" cidr="172.16.0.0/12"/>
      <node type="allow" cidr="127.0.0.0/8"/>
    </list>

    <list name="loopback.auto" default="allow">
      <node type="allow" cidr="127.0.0.0/8"/>
    </list>

    <list name="localnet.auto" default="allow">
    </list>

    <list name="nat.auto" default="allow">
    </list>

    <list name="domains" default="deny">
    </list>
  <list name="socket_acl" default="deny"><node type="allow" cidr="0.0.0.0/0"/></list><list name="socket_acl" default="deny"><node type="allow" cidr="0.0.0.0/0"/></list></network-lists>
</configuration>
